{
  "name": "agora-electron-sdk",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/__tests__/utils/"
    ]
  },
  "version": "2.2.1-alpha",
  "description": "agora-electron-sdk",
  "main": "js/AgoraSdk.js",
  "author": "agora.io",
  "scripts": {
    "install": "npm run build:electron",
    "build:node": "node ./scripts/install.js --runtime=node",
    "build:electron": "node ./scripts/install.js --runtime=electron",
    "debug": "node ./scripts/debug.js",
    "lint": "eslint .",
    "format": "eslint . --fix ",
    "test": "npm run build:node && jest"
  },
  "gypfile": true,
  "dependencies": {
    "chalk": "^2.4.1",
    "node-gyp": "^3.6.2",
    "optimist": "^0.6.1",
    "ora": "^2.1.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.22.2",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^23.1.0",
    "prettier": "^1.13.4"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron.git"
  },
  "keywords": [
    "electron",
    "agora",
    "sdk",
    "native"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron/issues"
  },
  "homepage": "https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron#readme"
}
