{
  "name": "agora-electron-sdk",
  "version": "2.3.2-alpha.1",
  "description": "agora-electron-sdk",
  "main": "js/AgoraSdk.js",
  "types": "types/AgoraSdk.d.ts",
  "author": "agora.io",
  "scripts": {
    "install": "npm run download",
    "build:dts": "rimraf ./types && tsc -p dtsconfig.json",
    "prebuild:ts": "npm run build:dts",
    "build:ts": "rimraf ./js && tsc -p tsconfig.json",
    "build:node": "ts-node ./scripts/build.ts --runtime=node",
    "build:electron2": "ts-node ./scripts/build.ts --runtime=electron --electron_version=1.8.3",
    "build:electron3": "ts-node ./scripts/build.ts --runtime=electron --electron_version=3.0.6",
    "build:electron4": "ts-node ./scripts/build.ts --runtime=electron --electron_version=4.0.0",
    "debug": "ts-node ./scripts/build.ts --runtime=electron --electron_version=3.0.6 --debug=true",
    "doc:js": "ts-node ./scripts/jsapi.ts",
    "download": "ts-node ./scripts/download.ts",
    "preexample": "npm run build:electron3",
    "example": "cd example && npm install && npm run dev",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "npm run build:node && jest --detectOpenHandles --forceExit",
    "quicktest": "jest --detectOpenHandles --forceExit"
  },
  "gypfile": true,
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "node-gyp": "^3.6.2",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "download": "^7.1.0",
    "jest": "^23.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "optimist": "^0.6.1",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.3",
    "semver": "^5.6.0",
    "shelljs": "^0.8.3",
    "signale": "^1.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {},
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron.git"
  },
  "keywords": [
    "electron",
    "agora",
    "sdk",
    "native"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron/issues"
  },
  "homepage": "https://github.com/AgoraIO-Community/Agora-RTC-SDK-for-Electron#readme"
}
